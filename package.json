{
    "name": "ai",
    "description": "Personalized AI assistant",
    "version": "0.0.0",
    "publisher": "efritz",
    "license": "MIT",
    "main": "./dist/extension.js",
    "bin": {
        "ai": "dist/cli.js"
    },
    "engines": {
        "vscode": "^1.92.0"
    },
    "activationEvents": [
        "onStartupFinished"
    ],
    "contributes": {
        "commands": [
            {
                "command": "aidev.chat",
                "title": "Open aidev"
            },
            {
                "command": "aidev.chat-model",
                "title": "Open aidev with specific model"
            },
            {
                "command": "aidev.chat-history",
                "title": "Open aidev from previous conversation"
            }
        ],
        "keybindings": [
            {
                "command": "aidev.chat",
                "key": "cmd+shift+i"
            }
        ]
    },
    "scripts": {
        "clean": "rm -rf dist ai-0.0.0.vsix",
        "build": "yarn clean && tsc --noEmit && node esbuild.config.js && sed -i '' 's/method: \"cancelled\"/method: \"notifications\\/cancelled\"/g' dist/cli.js",
        "dev": "yarn build && node dist/cli.js",
        "vsix": "yarn build && yarn vsce package --no-dependencies --allow-missing-repository",
        "format": "prettier '**/{*.{ts,json},.*.cjs}' --list-different --write",
        "lint": "eslint 'src/**/*.{js,ts}'",
        "test": "jest"
    },
    "dependencies": {
        "@anthropic-ai/sdk": "0.30.1",
        "@google/generative-ai": "0.21.0",
        "@humanwhocodes/gitignore-to-minimatch": "^1.0.2",
        "@modelcontextprotocol/sdk": "^1.0.0",
        "@types/diff": "^5.2.2",
        "@vscode/vsce": "^3.0.0",
        "chalk": "^5.3.0",
        "chokidar": "^3.6.0",
        "commander": "^12.1.0",
        "copy-paste": "^1.5.3",
        "diff": "^7.0.0",
        "eventsource": "^2.0.2",
        "glob": "^11.0.0",
        "groq-sdk": "0.7.0",
        "minimatch": "^10.0.1",
        "ollama": "0.5.9",
        "openai": "4.77.0",
        "ora": "^8.0.1",
        "tree-kill": "^1.2.2",
        "uuid": "^10.0.0"
    },
    "devDependencies": {
        "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
        "@types/copy-paste": "^1.1.33",
        "@types/eventsource": "^1.1.15",
        "@types/jest": "^29.5.12",
        "@types/minimatch": "^5.1.2",
        "@types/node": "^20.14.12",
        "@types/uuid": "^10.0.0",
        "@types/vscode": "^1.92.0",
        "@typescript-eslint/parser": "^8.2.0",
        "depcheck": "^1.4.7",
        "esbuild": "^0.23.1",
        "eslint": "^9.8.0",
        "eslint-plugin-unused-imports": "^4.0.1",
        "jest": "^29.7.0",
        "prettier": "^3.3.3",
        "ts-jest": "^29.1.2",
        "typescript": "^5.5.4"
    }
}
