{
    "name": "ai",
    "description": "Personalized AI assistant",
    "displayName": "aidev",
    "version": "0.0.0",
    "publisher": "efritz",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/efritz/aidev"
    },
    "main": "./dist/extension.cjs",
    "engines": {
        "vscode": "^1.92.0"
    },
    "activationEvents": [
        "onStartupFinished"
    ],
    "contributes": {
        "commands": [
            {
                "command": "aidev.chat",
                "title": "Open aidev"
            },
            {
                "command": "aidev.chat-history",
                "title": "Open aidev from previous conversation"
            }
        ],
        "keybindings": [
            {
                "command": "aidev.chat",
                "key": "cmd+shift+i"
            }
        ]
    },
    "scripts": {
        "dev": "bun ./src/cli.ts --cwd ${PWD}",
        "clean": "rm -rf ./dist ai-0.0.0.vsix",
        "build": "bun run ./scripts/build-extension.ts",
        "vsce-package": "bun run vsce package",
        "package": "bun run build && bun run vsce-package",
        "format": "prettier '**/{*.{ts,json},.*.cjs}' --list-different --write",
        "lint": "eslint 'src/**/*.{js,ts}'",
        "test": "jest"
    },
    "dependencies": {
        "@anthropic-ai/sdk": "0.33.1",
        "@google/generative-ai": "0.21.0",
        "@humanwhocodes/gitignore-to-minimatch": "^1.0.2",
        "@modelcontextprotocol/sdk": "1.4.1",
        "@types/diff": "^5.2.2",
        "@typescript-eslint/eslint-plugin": "^8.23.0",
        "@vscode/vsce": "^3.0.0",
        "chalk": "^5.3.0",
        "chokidar": "^3.6.0",
        "commander": "^12.1.0",
        "copy-paste": "^1.5.3",
        "diff": "^7.0.0",
        "eventsource": "^2.0.2",
        "glob": "^11.0.0",
        "groq-sdk": "0.9.1",
        "isbinaryfile": "^5.0.4",
        "minimatch": "^10.0.1",
        "ollama": "0.5.11",
        "openai": "4.77.0",
        "ora": "^8.0.1",
        "p-defer": "^4.0.1",
        "sqlite-vec": "^0.1.7-alpha.2",
        "tree-kill": "^1.2.2",
        "tree-sitter": "^0.22.4",
        "tree-sitter-go": "^0.23.4",
        "tree-sitter-typescript": "^0.23.2",
        "uuid": "^10.0.0",
        "yaml": "^2.7.0"
    },
    "devDependencies": {
        "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
        "@types/copy-paste": "^1.1.33",
        "@types/eventsource": "^1.1.15",
        "@types/jest": "^29.5.12",
        "@types/minimatch": "^5.1.2",
        "@types/node": "^20.14.12",
        "@types/uuid": "^10.0.0",
        "@types/vscode": "^1.92.0",
        "@typescript-eslint/parser": "^8.2.0",
        "bun-types": "^1.2.1",
        "depcheck": "^1.4.7",
        "esbuild": "^0.23.1",
        "eslint": "^9.8.0",
        "eslint-plugin-unused-imports": "^4.0.1",
        "jest": "^29.7.0",
        "prettier": "^3.3.3",
        "ts-jest": "^29.1.2",
        "typescript": "^5.5.4"
    }
}
